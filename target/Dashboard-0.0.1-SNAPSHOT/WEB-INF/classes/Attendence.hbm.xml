<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="Attendence" table="Attendence">
      <meta attribute="class-description">
         This class contains the employee detail. 
      </meta>
      <id name="employeeid" type="int" column="employeeid">
         <generator class="native"/>
      </id>
      <property name="managerid" column="managerid" type="string"/>
      <property name="scheduleid" column="scheduleid" type="string"/>
      <property name="attendance_t" column="attendance_t" type="int"/>
   </class>
</hibernate-mapping>